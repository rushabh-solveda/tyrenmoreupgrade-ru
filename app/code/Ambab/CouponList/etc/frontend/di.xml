		<arguments>
			<argument name="configProviders" xsi:type="array">
				<item name="coupon-list" xsi:type="object">Ambab\CouponList\Model\Checkout\ConfigProvider</item>
			</argument>
		</arguments>
	</type>
	<type name="Magento\Checkout\Block\Checkout\LayoutProcessor">
        <plugin name="ambab_couponlist_checkout_processor" type="Ambab\CouponList\Plugin\CheckoutProcessor" />
    </type>
</config>
